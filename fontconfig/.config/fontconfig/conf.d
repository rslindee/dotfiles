<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<fontconfig>
<!-- Fix the name of Terminus font from 'Terminus' to 'xos4 Terminus' -->
  <alias>
    <family>Terminus</family>
    <prefer><family>xos4 Terminus</family></prefer>
    <default><family>fixed</family></default>
  </alias>
</fontconfig>
